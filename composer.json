{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=5.6",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6.5",
        "dompdf/dompdf": "^0.8.1",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/address": "^1.7",
        "drupal/admin_toolbar": "^1.27",
        "drupal/advagg": "^4.0",
        "drupal/asset_injector": "~2.0",
        "drupal/better_exposed_filters": "^3.0",
        "drupal/bootstrap": "^3.19",
        "drupal/conditional_fields": "^1.0",
        "drupal/console": "^1.0.2",
        "drupal/content_sync": "^2.1",
        "drupal/contribute": "^1.0",
        "drupal/core": "^8.7||^9.0",
        "drupal/csv_serialization": "^1.4",
        "drupal/ctools": "^3.1",
        "drupal/dashboard": "^1.0",
        "drupal/devel": "^1.0",
        "drupal/devel_entity_updates": "^1.0",
        "drupal/draggableviews": "^1.2",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/embed": "^1.4",
        "drupal/empty_page": "~2.0",
        "drupal/entity": "^1.0",
        "drupal/entity_clone": "^1.0",
        "drupal/entity_embed": "^1.0",
        "drupal/entity_print": "^2.1",
        "drupal/entity_redirect": "^1.5",
        "drupal/entityqueue": "^1.0",
        "drupal/extlink": "^1.0",
        "drupal/field_group": "^3.0",
        "drupal/file_delete": "^1.0",
        "drupal/fivestar": "^1.0",
        "drupal/flag": "~4.0",
        "drupal/fontyourface": "^3.0",
        "drupal/geolocation": "^1.0",
        "drupal/google_analytics": "^2.3",
        "drupal/honeypot": "^1.28",
        "drupal/imce": "^1.7",
        "drupal/linkit": "^5.0",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/metatag": "^1.5",
        "drupal/mimemail": "^1.0",
        "drupal/pathauto": "^1.8",
        "drupal/php": "^1.0",
        "drupal/profile": "^1.0",
        "drupal/r4032login": "^1.1",
        "drupal/rate": "^1.0",
        "drupal/recently_read": "^1.0",
        "drupal/redirect": "^1.3",
        "drupal/rules": "^3.0",
        "drupal/seckit": "^1.0@alpha",
        "drupal/shs": "^1.0",
        "drupal/simple_sitemap": "^3.4",
        "drupal/structure_sync": "^1.16",
        "drupal/swiftmailer": "^1.0",
        "drupal/taxonomy_import": "^1.0",
        "drupal/term_csv_export_import": "^3.0@alpha",
        "drupal/term_csv_tree_import": "^1.0",
        "drupal/token": "^1.3",
        "drupal/typed_data": "^1.0",
        "drupal/views_data_export": "^1.0@RC",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/views_slideshow": "^4.7",
        "drupal/votingapi": "^3.0",
        "drupal/webform": "^5.12",
        "drupal/webform_score": "^1.0",
        "drupal/webform_views": "5.x-dev",
        "drupal/xmlsitemap": "^1.0",
        "drush/drush": "^9.7",
        "mikehaertl/phpwkhtmltopdf": "^2.2",
        "tecnickcom/tcpdf": "^6.2",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "wikimedia/composer-merge-plugin": "^1.4",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "^8.7.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-drupal-scaffold-cmd": [
            "git apply -v patches/core-htaccess_redirect_https_rule_s2s.patch"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        }
    }
}
